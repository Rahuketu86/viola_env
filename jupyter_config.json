{
    "VoilaConfiguration": {
        "enable_nbextensions": true,
        "template": "vuetify-default",
        "extension_language_mapping": {
            ".py": "python",
            ".jl": "julia"
        },
        "language_kernel_mapping": {
            "python": "python3"
        }
    };
    "TagRemovePreprocessor": {
        "remove_all_outputs_tags" = '{"viola-hide-output"}',
        "remove_cell_tags"='{"hide"}
    }
}
